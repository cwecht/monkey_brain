<?xml version="1.0"?>
<!-- test that onentry handlers are executed in document order.  event1 should be raised before event2 -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" xmlns:conf="http://www.w3.org/2005/scxml-conformance" conf:datamodel="" version="1.0">
  <state id="s0">
    <onentry>
      <raise event="event1"/>
    </onentry>
    <onentry>
      <raise event="event2"/>
    </onentry>
    <transition event="event1" target="s1"/>
    <transition event="*" target="conf:fail"/>
  </state>
  <state id="s1">
    <transition event="event2" target="conf:pass"/>
    <transition event="*" target="conf:fail"/>
  </state>
  <state id="conf:pass"/>
  <state id="conf:fail"/>
</scxml>
